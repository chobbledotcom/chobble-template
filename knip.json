{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		".eleventy.js",
		"src/_data/**/*.{js,mjs}",
		"src/**/*.11tydata.js",
		"src/_lib/public/bundle.js",
		"src/_lib/public/design-system.js",
		"bin/profile-report.js",
		"test/**/*.test.js",
		"test/build-profiling.js",
		"test/ensure-deps.js",
		"test/setup.js",
		"test/demo-precommit-errors.js"
	],
	"project": ["**/*.{js,mjs}"],
	"ignore": [
		"ecommerce-backend/**",
		"_site/**"
	],
	"ignoreDependencies": [
		"@zachleat/heading-anchors",
		"@11ty/eleventy-img",
		"bun-types",
		"json-to-pdf",
		"sass",
		"sharp"
	],
	"ignoreBinaries": [],
	"ignoreExportsUsedInFile": true,
	"rules": {},
	"eleventy": false
}
