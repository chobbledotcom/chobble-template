{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		".eleventy.js",
		"src/_data/**/*.{js,mjs}",
		"src/**/*.11tydata.js",
		"src/_lib/public/bundle.js"
	],
	"project": ["**/*.{js,mjs}"],
	"ignore": [
		"ecommerce-backend/**",
		"_site/**",
		"test/**",
		"src/_lib/public/theme/theme-editor-config.js"
	],
	"ignoreDependencies": [
		"@zachleat/heading-anchors",
		"@happy-dom/global-registrator",
		"happy-dom",
		"@adobe/structured-data-validator",
		"@marbec/web-auto-extractor",
		"@11ty/eleventy-img",
		"json-to-pdf",
		"liquidjs",
		"sass",
		"sharp"
	],
	"ignoreBinaries": [],
	"ignoreExportsUsedInFile": true,
	"rules": {},
	"eleventy": false
}
