{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		".eleventy.js",
		"test/**/*.js",
		"src/_data/**/*.{js,mjs}",
		"src/**/*.11tydata.mjs",
		"src/_js/**/*.js"
	],
	"project": ["**/*.{js,mjs}"],
	"ignore": [
		"src/assets/js/**",
		"ecommerce-backend/**",
		"_site/**"
	],
	"ignoreDependencies": [
		"@zachleat/heading-anchors"
	],
	"ignoreBinaries": ["biome"],
	"ignoreExportsUsedInFile": true,
	"rules": {
		"exports": "off"
	},
	"eleventy": false
}
