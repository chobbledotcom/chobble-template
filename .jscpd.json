{
	"threshold": 0,
	"reporters": ["console", "json"],
	"output": ".jscpd-report",
	"ignore": [
		"**/node_modules/**",
		"**/_site/**",
		"**/package-lock.json",
		"**/.test-sites/**",
		"**/page-layouts/**",
		"**/packages/**/index.js"
	],
	"format": ["javascript", "markup", "css", "scss"],
	"absolute": false,
	"gitignore": true,
	"exitCode": 1,
	"minLines": 1,
	"minTokens": 40,
	"path": ["src", "test", "scripts", ".eleventy.js"]
}
