{
	"threshold": 0,
	"reporters": ["console", "json"],
	"output": ".jscpd-report",
	"ignore": [
		"**/node_modules/**",
		"**/_site/**",
		"**/package-lock.json",
		"**/ecommerce-backend/**",
		"**/.test-sites/**",
		"**/landing-pages/**",
		"**/page-layouts/**",
		"**/browser-utils.js",
		"**/lighthouse.js",
		"**/screenshot.js",
		"**/cli-utils.js"
	],
	"format": ["javascript", "json", "markup", "css", "scss"],
	"absolute": false,
	"gitignore": true,
	"exitCode": 1,
	"minLines": 1,
	"minTokens": 40,
	"path": ["src", "test", "scripts", ".eleventy.js"]
}
