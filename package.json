{
	"name": "chobble-template",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"build": "rm -rf _site && eleventy",
		"serve": "rm -rf _site && eleventy --serve --incremental --quiet",
		"test": "node test/run-all-tests.js",
		"cpd": "jscpd",
		"knip": "knip",
		"knip:fix": "knip --fix",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"precommit": "run-s lint:fix cpd knip:fix test"
	},
	"devDependencies": {
		"esbuild": "^0.27.0",
		"jscpd": "^4.0.5",
		"knip": "^5.38.0",
		"npm-run-all2": "^8.0.4"
	},
	"dependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-img": "^6.0.0",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.2",
		"@botpoison/browser": "^0.1.30",
		"@hotwired/turbo": "^8.0.20",
		"@quasibit/eleventy-plugin-schema": "^1.11.1",
		"@zachleat/heading-anchors": "^1.0.1",
		"fast-glob": "^3.3.2",
		"ical-generator": "^9.0.0",
		"jsdom": "^26.1.0",
		"sass": "^1.86.3"
	}
}
